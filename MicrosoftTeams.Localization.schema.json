{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string",
      "format": "uri"
    },
    "name.short": {
      "type": "string",
      "maxLength": 30
    },
    "name.full": {
      "type": "string",
      "maxLength": 100
    },
    "description.short": {
      "type": "string",
      "maxLength": 80
    },
    "description.full": {
      "type": "string",
      "maxLength": 4000
    }
  },
  "patternProperties": {
    "^staticTabs\\[([0-9]|1[0-5])\\]\\.name$": {
      "type": "string",
      "maxLength": 128
    },
    "^bots\\[0\\]\\.commandLists\\[[0-1]\\]\\.commands\\[[0-9]\\]\\.title$": {
      "type": "string",
      "maxLength": 32
    },
    "^bots\\[0\\]\\.commandLists\\[[0-1]\\]\\.commands\\[[0-9]\\]\\.description$": {
      "type": "string",
      "maxLength": 128
    },
    "^composeExtensions\\[0\\]\\.commands\\[[0-9]\\]\\.title$": {
      "type": "string",
      "maxLength": 32
    },
    "^composeExtensions\\[0\\]\\.commands\\[[0-9]\\]\\.description$": {
      "type": "string",
      "maxLength": 128
    },
    "^composeExtensions\\[0\\]\\.commands\\[[0-9]\\]\\.parameters\\[[0-4]\\]\\.title$": {
      "type": "string",
      "maxLength": 32
    },
    "^composeExtensions\\[0\\]\\.commands\\[[0-9]\\]\\.parameters\\[[0-4]\\]\\.description$": {
      "type": "string",
      "maxLength": 128
    }
  }
}
